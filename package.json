{
  "name": "@rtobon/rumba",
  "version": "1.5.3",
  "description": "Dead-simple API actions for Redux",
  "repository": {
    "type": "git",
    "url": "https://github.com/nartobon/rumba.git"
  },
  "author": "Oleg Orlov <mail@orlovoleg.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "es"
  ],
  "scripts": {
    "preversion": "npm run clean && npm run check && npm run build",
    "clean": "rimraf dist es",
    "lint": "eslint src",
    "test": "cross-env BABEL_ENV=commonjs ava",
    "check": "npm run lint && npm run test",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir dist",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:commonjs && npm run build:es"
  },
  "dependencies": {
    "immutable": "3.8.1",
    "lodash.mapvalues": "4.6.0",
    "lodash.merge": "4.6.0",
    "normalizr": "2.2.1",
    "reduce-reducers": "0.1.2",
    "redux-actions": "0.13.0",
    "superagent": "2.3.0"
  },
  "devDependencies": {
    "@rtobon/babel-preset-eagle": "1.0.5",
    "@rtobon/eslint-config-eagle": "1.4.2",
    "ava": "0.16.0",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-register": "6.18.0",
    "cross-env": "3.1.3",
    "eslint": "3.9.1",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "rimraf": "2.5.4"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  }
}
